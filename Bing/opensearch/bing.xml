<?xml version="1.0" encoding="utf-8"?>
<OpenSearchDescription xmlns="http://a9.com/-/spec/opensearch/1.1/" xmlns:moz="http://www.mozilla.org/2006/browser/search/">
	<ShortName>Bing</ShortName>
	<LongName>Bing Search</LongName>
	<Description>Bing で検索</Description>
	<Image height="16" width="16" type="image/x-icon">../images/favicon.ico</Image>
	<Image height="64" width="64" type="image/png">../images/icon.png</Image>
	<InputEncoding>UTF-8</InputEncoding>
	<OutputEncoding>UTF-8</OutputEncoding>
	<Url type="text/html" rel="results" method="get" template="https://bing.com/search">
		<Param name="q" value="{searchTerms}" />
		<Param name="scope" value="web" />
		<Param name="mkt" value="ja-jp" />
		<Param name="cc" value="jp" />
		<Param name="setLang" value="ja-jp" />
		<Param name="textDecoration" value="false" />
	</Url>
	<Url type="application/x-suggestions+json" method="get" template="https://www.bing.com/osjson.aspx">
		<Param name="query" value="{searchTerms}" />
		<Param name="mkt" value="ja-jp" />
		<Param name="cc" value="jp" />
	</Url>
	<Url type="application/opensearchdescription+xml" rel="self" template="https://akimikimikimikimikimikimika.github.io/Search/Bing/opensearch/bing.xml" />
	<moz:SearchForm>https://bing.com/?scope=web&amp;mkt=ja-jp&amp;cc=jp&amp;setLang=ja-jp</moz:SearchForm>
</OpenSearchDescription>